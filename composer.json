{
    "name": "mrden/pdf-version-converter",
    "description": "PHP library for converting the version of PDF files (for compatibility purposes).",
    "keywords": ["pdf", "converter", "version converter", "pdf version converter", "pdf version"],
    "license": "MIT",
    "type": "library",
    "minimum-stability" : "stable",
    "authors": [
        {
            "name": "Thiago Rodrigues",
            "email": "xthiago@gmail.com"
        },
        {
            "name": "Denis Kiselev",
            "email": "kiselevden911@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.3",
        "symfony/filesystem": "^5.4",
        "symfony/process": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Mrden\\PDFVersionConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
